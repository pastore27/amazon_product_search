<div class="container">
  <table class="table table-striped table-hover">
    <thead>
      <tr>
        <th>Titile</th>
        <th>Price</th>
        <th>Headline</th>
        <th></th>
        <th></th>
        <th></th>
      </tr>
    </thead>
    <tbody id="tbody">
      <% @items.each do |item| %>
      <tr>
        <td><a href="<%= item['url'] %>" target="_blank"><%= item['title'] %></a></td>
        <td><%= item['price'] %></td>
        <td><%= item['headline'] %></td>
        <td></td>
        <td></td>
        <td></td>
      </tr>
      <% end %>
    </tbody>
  </table>

  <button type="submit" class="btn btn-primary">検索条件を保存</button>
</div>
